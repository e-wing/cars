<p id="notice"><%= notice %></p>

<h1>Listing Sample Finishes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sample_finishes.each do |sample_finish| %>
      <tr>
        <td><%= link_to 'Show', sample_finish %></td>
        <td><%= link_to 'Edit', edit_sample_finish_path(sample_finish) %></td>
        <td><%= link_to 'Destroy', sample_finish, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sample finish', new_sample_finish_path %>
